[
  {
    "bindings": {
      "shift-tab": "workspace::ToggleLeftDock",
      "cmd-shift-c": "workspace::ToggleBottomDock",
      "cmd-r": "project_panel::ToggleFocus",
      "alt-p": ["task::Spawn", { "task_name": "Example task" }]
    }
  },
  {
    "context": "Editor && vim_mode == insert",
    "bindings": {
      "j j": "vim::NormalBefore"
    }
  },
  {
    "context": "Editor",
    "bindings": {
      "cmd-e": "editor::Rename",
      "cmd-w": "editor::SelectLargerSyntaxNode",
      "cmd-shift-w": "editor::SelectSmallerSyntaxNode"
    }
  },
  {
    "context": "ProjectPanel",
    "bindings": {
       "cmd-e": "project_panel::Rename"
    }
  },
  {
    "context": "Editor && vim_mode == normal && (vim_operator == none || vim_operator == n) && !VimWaiting",
    "bindings": {
      "g d": "editor::GoToDefinition",
      "g u": "editor::FindAllReferences",
      "[ d": "editor::GoToPrevDiagnostic",
      "] d": "editor::GoToDiagnostic"
    }
  },
  {
    "context": "Terminal",
    "bindings": {
      "cmd-t": "workspace::NewTerminal"
    }
  }
]
